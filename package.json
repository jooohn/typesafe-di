{
  "name": "typesafe-di",
  "version": "0.3.1",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/m3dev/typesafe-di.git",
  "author": "jooohn <jooohn12341234@gmail.com>",
  "license": "MIT",
  "keywords": [
    "di",
    "ioc",
    "dependency injection",
    "dependency inversion",
    "inversion of control"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run tsd && npm run lint",
    "preversion": "npm run build && npm run test && npm run tsd && npm run lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "tsd": "tsd",
    "format": "prettier --write \"src/**/*.ts\" && eslint --fix \"src/**/*.ts\"",
    "lint": "eslint \"src/**/*.ts\""
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.59.9",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "tsd": "^0.28.1",
    "typescript": "^4.3"
  },
  "files": [
    "lib/**/*"
  ],
  "jest": {
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "node_modules",
      "typing-test"
    ]
  },
  "dependencies": {}
}
